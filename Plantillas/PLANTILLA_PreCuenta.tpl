<table cellpadding="0" cellspacing="0" style="border-color:#e5e5e5">
	<tbody>
		<tr>
			<td colspan="6" style="text-align:center"><span style="font-size:11px"><strong>{Empresa}</strong></span></td>
		</tr>
		<tr>
			<td colspan="6" style="text-align:center"><span style="font-size:11px"><strong>NIT: {NIT} {Regimen}</strong></span></td>
		</tr>
		<tr>
			<td colspan="6" style="text-align:center"><span style="font-size:11px">{DireccionEmpresa} Tel&eacute;fonos: {TelefonoEmpresa} Ciudad: {CiudadEmpresa}</span></td>
		</tr>
		<tr>
			<td colspan="6" style="text-align:center">&nbsp;</td>
		</tr>
		<tr>
			<td colspan="2"><span style="font-size:11px"><strong>Datos del Cliente:</strong></span></td>
			<td colspan="3" rowspan="2" style="text-align:center"><span style="font-size:11px"><strong>PRE CUENTA</strong></span></td>
		</tr>
		<tr>
			<td colspan="2"><span style="font-size:11px">C&eacute;dula o Nit: {Documento}</span></td>
		</tr>
		<tr>
			<td colspan="2"><span style="font-size:11px">Nombre.: {Titular}</span></td>
			<td colspan="3" rowspan="2" style="text-align:center"><span style="font-size:11px">Fecha y Hora: {FechaHora}</span></td>
		</tr>
		<tr>
			<td colspan="2"><span style="font-size:11px">Direc.: {Direccion}</span></td>
		</tr>
		<tr>
			<td colspan="2"><span style="font-size:11px">Tel&eacute;f: {Telefono}</span></td>
			<td colspan="3" style="text-align:right">&nbsp;</td>
		</tr>
		<tr>
			<td colspan="2" rowspan="1"><span style="font-size:11px">Zona: {Zona}</span></td>
			<td colspan="3"><span style="font-size:11px">Vendedor: {Vendedor}</span></td>
		</tr>
		<tr>
			<td colspan="5">
			<hr /></td>
		</tr>
		<tr>
			<td><span style="font-size:11px">Regi&oacute;n: {Region}</span></td>
			<td><span style="font-size:11px">Ciudad: {Ciudad}</span></td>
			<td colspan="3" rowspan="1"><span style="font-size:11px">Barrio: {Barrio}</span></td>
		</tr>
		<tr>
			<td colspan="5" rowspan="1">
			<hr /></td>
		</tr>
		<tr>
			<td colspan="5" rowspan="1">
			<p><span style="font-size:11px">[</span></p>

			<table cellpadding="0" cellspacing="0" style="border-color:#e5e5e5; width:100%">
				<thead>
					<tr>
						<th style="width:5%"><span style="font-size:11px"><span style="font-size:11px">Item</span></span></th>
						<th><span style="font-size:11px"><span style="font-size:11px">Descripci&oacute;n del Producto</span></span></th>
						<th style="width:5%"><span style="font-size:11px"><span style="font-size:11px">Cantidad</span></span></th>
						<th style="width:5%"><span style="font-size:11px"><span style="font-size:11px">Unidad Medida</span></span></th>
						<th style="width:5%"><span style="font-size:11px"><span style="font-size:11px">Precio Unitario</span></span></th>
						<th style="width:5%"><span style="font-size:11px"><span style="font-size:11px">Descuento</span></span></th>
						<th style="width:5%"><span style="font-size:11px"><span style="font-size:11px">Valor Total</span></span></th>
						<th style="width:10%"><span style="font-size:11px"><span style="font-size:11px">IVA %</span></span></th>
						<th><span style="font-size:11px"><span style="font-size:11px">Observaci&oacute;n</span></span></th>
					</tr>
				</thead>
				<tbody>
					<tr>
						<td><span style="font-size:11px"><span style="font-size:11px">{CodiP}</span></span></td>
						<td><span style="font-size:11px"><span style="font-size:11px">{NombreP}</span></span></td>
						<td><span style="font-size:11px"><span style="font-size:11px">{CantiP}</span></span></td>
						<td><span style="font-size:11px"><span style="font-size:11px">{UnidadP}</span></span></td>
						<td><span style="font-size:11px"><span style="font-size:11px">{ValorUniP}</span></span></td>
						<td><span style="font-size:11px"><span style="font-size:11px">{DctoP}</span></span></td>
						<td><span style="font-size:11px"><span style="font-size:11px">{ValorTotalP}</span></span></td>
						<td><span style="font-size:11px"><span style="font-size:11px">{IvaP}</span></span></td>
						<td><span style="font-size:11px"><span style="font-size:11px">{ObservacionP}</span></span></td>
					</tr>
				</tbody>
			</table>

			<p><span style="font-size:11px">]</span></p>
			</td>
		</tr>
		<tr>
			<td colspan="5" rowspan="1">
			<hr /></td>
		</tr>
		<tr>
			<td colspan="2" style="text-align:center"><span style="font-size:11px"><strong>Cantidad Productos:</strong> {Cantidad}</span></td>
			<td colspan="2" rowspan="2" style="text-align:right"><span style="font-size:11px"><strong>Sub Total:</strong></span></td>
			<td colspan="1" rowspan="2" style="text-align:right"><span style="font-size:11px">{SubTotal}</span></td>
		</tr>
		<tr>
			<td colspan="2">&nbsp;</td>
		</tr>
		<tr>
			<td colspan="2">&nbsp;</td>
			<td colspan="2" style="text-align:right"><span style="font-size:11px"><strong>Descuento:</strong></span></td>
			<td colspan="1" style="text-align:right"><span style="font-size:11px">{Descuento}</span></td>
		</tr>
		<tr>
			<td colspan="2" rowspan="4">&nbsp;</td>
			<td colspan="2" style="text-align:right"><span style="font-size:11px"><strong>Val. Antes Iva:</strong></span></td>
			<td colspan="1" style="text-align:right"><span style="font-size:11px">{AntesIVA}</span></td>
		</tr>
		<tr>
			<td colspan="2" style="text-align:right"><span style="font-size:11px"><strong>IVA:</strong></span></td>
			<td colspan="1" style="text-align:right"><span style="font-size:11px">{Iva}</span></td>
		</tr>
		<tr>
			<td colspan="2" style="text-align:right"><span style="font-size:11px"><strong>ImpoConsumo</strong>:</span></td>
			<td colspan="1" style="text-align:right"><span style="font-size:11px">{ImpoConsumo}</span></td>
		</tr>
		<tr>
			<td colspan="2" style="text-align:right"><span style="font-size:11px"><strong>Total Factura:</strong></span></td>
			<td colspan="1" style="text-align:right"><strong><span style="font-size:11px">{TotalFactura}</span></strong></td>
		</tr>
		<tr>
			<td colspan="5" rowspan="1">
			<hr /></td>
		</tr>
		<tr>
			<td colspan="2"><strong><span style="font-size:11px">(Nombre y Firma)</span></strong></td>
			<td>&nbsp;</td>
			<td>&nbsp;</td>
			<td>&nbsp;</td>
		</tr>
	</tbody>
</table>
